                                    </tr>
                                    <tr>
                                        <td>003</td>
                                        <td>Otto Clay</td>
                                        <td>23</td>
                                        <td>China</td>
                                        <td class="nv-table-text-right">23.00</td>
                                        <td><a>Edit</a><a>Delete</a></td>
                                    </tr>
                                    <tr>
                                        <td>004</td>
                                        <td>Otto Clay</td>
                                        <td>23</td>
                                        <td>China</td>
                                        <td class="nv-table-text-right">23.00</td>
                                        <td><a>Edit</a><a>Delete</a></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <!--内容区域-->
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{% include 'src/tmpl/include/footer.nv' %}